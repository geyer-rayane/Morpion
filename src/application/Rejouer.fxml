<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/17.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Button fx:id="rejouerButton" layoutX="100.0" layoutY="80.0" mnemonicParsing="false" onAction="#rejouerAction" prefHeight="40.0" prefWidth="100.0" text="Rejouer">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
        <Button fx:id="accueilButton" layoutX="100.0" layoutY="140.0" mnemonicParsing="false" onAction="#accueilAction" prefHeight="40.0" prefWidth="100.0" text="Accueil">
            <font>
                <Font size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
